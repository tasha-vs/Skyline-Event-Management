
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Client List:</h2>
<br />
<table class="table">
    <tr>
        <th> Email </th>
        <th> Full Name </th>
        <th> Address </th>
        <th> Age </th>
        <th> Phone </th>
    </tr>
    @foreach (var i in Model)
    {
    <tr>
        <td> @i.Email </td>
        <td> @i.FullName </td>
        <td> @i.Address </td>
        <td> @i.Age </td>
        <td> @i.Phone </td>
    </tr>
    }
</table>

